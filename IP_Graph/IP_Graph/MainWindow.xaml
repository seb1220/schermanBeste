<Window x:Class="IP_Graph.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IP_Graph"
        xmlns:GraphControl="clr-namespace:GraphControl;assembly=GraphControl"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <GraphControl:Graph StartChanged="GraphComponent_StartChanged" EndChanged="GraphComponent_EndChanged" Name="GraphComponent"/>
    </Grid>
</Window>
